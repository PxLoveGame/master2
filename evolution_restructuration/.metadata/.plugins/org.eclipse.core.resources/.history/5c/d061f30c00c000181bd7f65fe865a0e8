
public class Animal {
	
	private String name;
	private String breed;
	private String age;
	
	
	public String getName(){
		return this.name;
	}
	
	public String getAge(){
		return this.age;
	}
	
	public String getBreed(){
		return this.breed;
	}
	
	private void setBreed(String breed){
		this.breed = breed;
	}
	
}
